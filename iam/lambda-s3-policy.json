{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ReadFromIntakeBucket",
      "Effect": "Allow",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::scrapper-bucket6767/*"
    },
    {
      "Sid": "WriteToQuarantineBucket",
      "Effect": "Allow",
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::quarentine-bucket6767/*"
    }
  ]
}
